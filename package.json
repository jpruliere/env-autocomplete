{
	"name": "env-autocomplete",
	"displayName": "env-autocomplete",
	"description": "Searches your repo for a .env file, parses it and autocompletes process.env mentions with every available variable",
	"version": "1.3.0",
	"publisher": "jpruliere",
	"repository": {
		"type": "git",
		"url": "https://github.com/jpruliere/env-autocomplete"
	},
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:typescript",
		"onLanguage:javascript",
		"onLanguage:typescriptreact",
		"onLanguage:javascriptreact"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint src --ext ts",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "^17.0.23",
		"@types/vscode": "^1.65.0",
		"eslint": "^8.12.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.6.3"
	}
}

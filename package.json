{
	"name": "env-autocomplete",
	"displayName": "env-autocomplete",
	"description": "Searches your repo for a .env file, parses it and autocompletes process.env mentions with every available variable",
	"version": "1.3.0",
	"publisher": "jpruliere",
	"repository": {
		"type": "git",
		"url": "https://github.com/jpruliere/env-autocomplete"
	},
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:typescript",
		"onLanguage:javascript",
		"onLanguage:typescriptreact",
		"onLanguage:javascriptreact"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint src --ext ts",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.49.0",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode": "^1.1.37",
		"vscode-test": "^1.4.0"
	}
}
